{
  "name": "typo3/t3extblog",
  "description": "A flexible blog extension powered by Extbase / Fluid which aims to replace t3blog.",
  "require-dev": {
    "fluidtypo3/development": "3.1.5"
  },
  "autoload": {
    "psr-4": {
      "TYPO3\\T3extblog\\": "Classes/"
    }
  },
  "type": "typo3-cms-extension",
  "keywords": ["TYPO3 CMS", "blog"],
  "homepage": "https://github.com/fnagel/t3extblog",
  "license": "GPL-2.0+",
  "version": "2.2.2-dev",
  "support": {
    "issues": "https://github.com/fnagel/t3extblog/issues"
  },
  "require": {
    "php": ">=5.5,<7.2",
    "typo3/cms-core": ">=7.3.0,<=8.7.99"
  },
  "conflict": {
    "dmitryd/typo3-realurl": ">=2.0.0 <=2.0.10"
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/TYPO3/TYPO3.CMS"
    }
  ]
}
