{
  "name": "felixnagel/t3extblog",
  "description": "A record based blog extension for TYPO3 CMS powered by Extbase / Fluid. Flexible and powerful!",
  "type": "typo3-cms-extension",
  "keywords": ["TYPO3 CMS", "blog"],
  "homepage": "https://github.com/fnagel/t3extblog",
  "license": "GPL-2.0+",
  "support": {
    "issues": "https://github.com/fnagel/t3extblog/issues"
  },
  "authors": [
	{
	  "name": "Felix Nagel",
	  "email": "info@felixnagel.com",
	  "homepage": "http://www.felixnagel.com",
	  "role": "Developer"
	}
  ],
  "require": {
    "php": ">=7.0,<7.3",
    "typo3/cms-core": ">=9.0.0,<=9.5.99"
  },
  "require-dev": {
    "nimut/testing-framework": "^3.0"
  },
  "autoload": {
    "psr-4": {
      "FelixNagel\\T3extblog\\": "Classes"
    }
  },
  "autoload-dev": {
	"psr-4": {
	   "FelixNagel\\T3extblog\\Tests\\": "Tests"
	}
  },	
  "conflict": {
    "typo3-ter/t3blog": ">=0.0.1",
    "dmitryd/typo3-realurl": ">=2.0.0,<=2.0.10"
  },
  "suggest": {
    "dmitryd/typo3-dd-googlesitemap": "~2.0",
    "dmitryd/typo3-realurl": "~2.0"
  },
  "replace": {
    "t3extblog": "self.version",
    "typo3-ter/t3extblog": "self.version"
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/TYPO3/TYPO3.CMS"
    }
  ],
  "extra": {
    "typo3/cms": {
      "extension-key": "t3extblog"
    }
  }
}
